{
  "name": "cutten-web",
  "version": "1.0.0",
  "description": "Front end for whatsapp chat analyzer.",
  "author": "Adam Aho & Brett Bullock",
  "license": "MIT",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "concurrently -r \"npm:type-check:watch\" \"npm:start\"",
    "start": "parcel ./src/index.html --port 9000",
    "build": "npm run type-check && parcel build ./src/index.html --public-url '/'",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "lint": "tslint 'src/**/*.ts?(x)' --quiet",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "@types/apollo-upload-client": "^8.1.2",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.6",
    "@types/react": "16.8.24",
    "@types/react-dom": "^16.8.4",
    "concurrently": "^4.1.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-dom": "^3.1.3",
    "jest-extended": "^0.11.1",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^6.1.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.4"
  },
  "dependencies": {
    "@types/styled-components": "^4.1.18",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link-http": "^1.5.15",
    "apollo-upload-client": "^11.0.0",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "kingsbury": "^0.0.58",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6"
  }
}
